{
  "name": "musa-server",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "shx mkdir dist && shx touch dist/index.js",
    "dev": "env-cmd -f .env.dev nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env node dist/index.js",
    "build:mac": "env-cmd -f .env sh scripts/build.mac.sh",
    "build:nux": "env-cmd -f .env sh scripts/build.mac.sh",
    "build:win": "env-cmd -f .env sh scripts/build.win.bat",
    "build:backend": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit",
    "test": "vitest run --coverage",
    "test:w": "vitest",
    "isvc": "env-cmd -f .env node scripts/isvc.js",
    "uisvc": "env-cmd -f .env node scripts/uisvc.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@miikaah/musa-core": "10.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.2",
    "@types/supertest": "^2.0.14",
    "@types/urlsafe-base64": "^1.0.29",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^0.34.6",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
