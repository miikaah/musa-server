{
  "name": "musa-server",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "shx mkdir dist && shx touch dist/index.js",
    "dev": "env-cmd -f .env.dev nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env node dist/index.js",
    "build:mac": "env-cmd -f .env sh scripts/build.mac.sh",
    "build:nux": "env-cmd -f .env sh scripts/build.mac.sh",
    "build:win": "env-cmd -f .env sh scripts/build.win.bat",
    "build:backend": "esbuild src/index.ts --bundle --platform=node --target=node22 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit",
    "test": "vitest run --coverage",
    "test:w": "vitest",
    "isvc": "env-cmd -f .env node scripts/isvc.js",
    "uisvc": "env-cmd -f .env node scripts/uisvc.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@miikaah/musa-core": "10.1.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "env-cmd": "10.1.0",
    "express": "5.0.0-beta.3"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.10",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "@vitest/coverage-v8": "1.6.0",
    "esbuild": "0.21.5",
    "eslint": "9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "nodemon": "3.1.4",
    "prettier": "3.3.2",
    "prettier-plugin-organize-imports": "4.0.0",
    "shx": "0.3.4",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.3",
    "vitest": "1.6.0"
  }
}
