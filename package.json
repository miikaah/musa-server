{
  "name": "musa-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch ./src --ext ts --exec npm run start",
    "start": "env-cmd ts-node --transpile-only src/index.ts",
    "tsc:w": "tsc --watch --noEmit",
    "test:w": "jest --watch",
    "db:setup": "docker-compose up -d",
    "db": "psql -h localhost -d musa -p 5432 -U pg",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "env-cmd knex migrate:latest",
    "migrate:rollback": "env-cmd knex migrate:rollback"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "fuzzysort": "^1.1.4",
    "knex": "^0.95.11",
    "music-metadata": "^7.11.4",
    "pg": "^8.7.1",
    "ts-node": "^10.2.1",
    "urlsafe-base64": "^1.0.0",
    "musa-core": "git@github.com:miikaah/musa-core.git#a1fb15c063f0cec8d432447f7d954a0face2164e"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.1",
    "@types/supertest": "^2.0.11",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "nodemon": "2.0.7",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  }
}
