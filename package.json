{
  "name": "musa-server",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "shx mkdir dist && shx touch dist/index.js",
    "dev": "env-cmd -f .env.dev nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env node dist/index.js",
    "build": "shx rm dist/index.js && npm run build:backend",
    "build:backend": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit",
    "test": "jest --coverage",
    "test:w": "jest --watch",
    "test:heap": "jest --coverage --runInBand --logHeapUsage",
    "test:debug": "node --inspect-brk --expose-gc ./node_modules/.bin/jest",
    "isvc": "env-cmd -f .env node scripts/isvc.js",
    "uisvc": "env-cmd -f .env node scripts/uisvc.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@miikaah/musa-core": "9.4.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@swc/core": "^1.3.69",
    "@swc/jest": "^0.2.26",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/supertest": "^2.0.12",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.18.13",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.3",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
