{
  "name": "musa-server",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "shx mkdir dist && shx touch dist/index.js",
    "dev": "env-cmd -f .env.dev nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env node dist/index.js",
    "build": "shx rm dist/index.js && npm run build:backend",
    "build:backend": "esbuild src/index.ts --bundle --platform=node --target=node16 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit",
    "test": "jest --coverage",
    "test:w": "jest --watch",
    "test:heap": "jest --coverage --runInBand --logHeapUsage",
    "test:debug": "node --inspect-brk --expose-gc ./node_modules/.bin/jest",
    "isvc": "qckwinsvc --name MusaServer --script dist\\index.js --description MusaServer --startImmediately",
    "uisvc": "qckwinsvc --uninstall --name MusaServer --script dist\\index.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@miikaah/musa-core": "8.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "shx": "^0.3.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
