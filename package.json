{
  "name": "musa-server",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "env-cmd -f .env nodemon --watch ./src --ext ts --exec ts-node --transpile-only src/index.ts",
    "start": "env-cmd -f .env.prod node dist/index.js",
    "build": "shx rm -rf dist && esbuild src/index.ts --bundle --platform=node --target=node16 --outdir=dist",
    "tsc:w": "tsc --watch --noEmit",
    "test": "jest --coverage",
    "test:w": "jest --watch",
    "test:heap": "jest --coverage --runInBand --logHeapUsage",
    "test:debug": "node --inspect-brk --expose-gc ./node_modules/.bin/jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@miikaah/musa-core": "7.10.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^5.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "esbuild": "^0.15.14",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
